    "@types/react-dom": "^18.0.10",
    "@types/topojson-client": "^3.1.1",
    "gh-pages": "^5.0.0",
    "react-scripts": "^5.0.1",
    "tsc-esm": "^1.0.4",
    "tsc-esm-fix": "^2.20.12",
    "typedoc": "^0.23.25",
